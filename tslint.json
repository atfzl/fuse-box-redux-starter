{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "uppercase-first",
        "named-imports-order": "any"
      }
    ]
  }
}
